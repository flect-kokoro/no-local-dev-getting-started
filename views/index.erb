<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>deviceName</th>
      <th>id</th>
      <th>SFID</th>
    </tr>

    <% @devices.each do |account| %>
      <tr>
        <td><%= account.name %></td>
        <td><%= account.id %></td>
        <td><%= account.sfid %></td>
      </tr>
    <% end %>
  </table>

</div>
