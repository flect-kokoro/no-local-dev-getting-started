<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>

    <% @devices.each do |device| %>
      <tr>
        <td><%= device.Name %></td>
        <td><%= device.Id %></td>
        <td><%= device.SFID %></td>
      </tr>
    <% end %>
  </table>

</div>
